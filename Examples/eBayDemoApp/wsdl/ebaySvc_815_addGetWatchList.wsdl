					</xs:documentation>
       <xs:appinfo>
        <CallInfo>
         <CallName>GetMyeBayBuying</CallName>
         <Context>BestOfferList</Context>
         <RequiredInput>No</RequiredInput>
         <OnlyTheseValues>Auction, FixedPriceItem, AdType</OnlyTheseValues>
        </CallInfo>
